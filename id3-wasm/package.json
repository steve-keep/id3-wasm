{
  "name": "id3-wasm",
  "version": "0.1.0",
  "description": "Insanely quick ID3 reading & writing for JavaScript powered by WebAssembly.",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "lint": "eslint src/**/*.ts",
    "build:ts": "esbuild src/index.ts --bundle --outfile=dist/index.js --format=esm --platform=browser --external:*.wasm"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/steve-keep/id3-wasm.git"
  },
  "keywords": [
    "webassembly",
    "wasm",
    "rust",
    "id3"
  ],
  "bugs": {
    "url": "https://github.com/steve-keep/id3-wasm/issues"
  },
  "homepage": "https://github.com/steve-keep/id3-wasm#readme",
  "files": [
    "wasm",
    "dist"
  ],
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^8.50.0",
    "@typescript-eslint/parser": "^8.50.0",
    "esbuild": "^0.27.2",
    "eslint": "^8.57.1",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-promise": "^7.2.1",
    "typedoc": "^0.25.13",
    "typescript": "^5.9.3"
  }
}
